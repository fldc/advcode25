10 REM ADVENT OF CODE DAY 4 - PART 1
20 PRINT CHR$(147)
25 PRINT "ADVENT OF CODE DAY 4 : PART 1"
30 W=140:H=138
40 DIM G$(H)
50 AC=0
60 PRINT "READING INPUT..."
100 FOR I=1 TO H
110 READ G$(I)
120 NEXT I
130 PRINT "I'M ROLLIN, ROLLIN, ROLLIN..."
150 FOR Y=1 TO H
160 FOR X=1 TO W
180 IF MID$(G$(Y),X,1)<>"@" THEN 350
200 N=0
210 FOR DY=-1 TO 1
220 FOR DX=-1 TO 1
230 IF DX=0 AND DY=0 THEN 320
240 NY=Y+DY:NX=X+DX
250 IF NY<1 OR NY>H THEN 320
260 IF NX<1 OR NX>W THEN 320
270 IF MID$(G$(NY),NX,1)="@" THEN N=N+1
320 NEXT DX
330 NEXT DY
340 IF N<4 THEN AC=AC+1
350 NEXT X
360 NEXT Y
370 PRINT "ACCESSIBLE ROLLS: ";AC
380 END
